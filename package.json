{
    "name": "snatcher-backend",
    "version": "1.0.0",
    "description": "Backend API for Snatched virtual try-on app",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "nodemon --watch src --exec tsx src/server.ts",
        "build": "tsc",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "tsx src/migrate.ts",
        "db:studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.956.0",
        "@aws-sdk/s3-request-presigner": "^3.956.0",
        "@google/genai": "^1.34.0",
        "@google/generative-ai": "^0.21.0",
        "@imgly/background-removal-node": "^1.4.5",
        "@upstash/redis": "^1.35.8",
        "axios": "^1.7.9",
        "backblaze-b2": "^1.7.0",
        "cheerio": "^1.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "drizzle-orm": "^0.36.4",
        "express": "^4.21.2",
        "express-rate-limit": "^8.2.1",
        "google-auth-library": "^10.5.0",
        "helmet": "^8.1.0",
        "https-proxy-agent": "^7.0.6",
        "multer": "^1.4.5-lts.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "postgres": "^3.4.5",
        "puppeteer": "^24.32.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "sharp": "^0.34.5",
        "user-agents": "^1.1.669",
        "uuid": "^11.0.3",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.2",
        "@types/sharp": "^0.31.1",
        "@types/user-agents": "^1.0.4",
        "@types/uuid": "^10.0.0",
        "drizzle-kit": "^0.30.1",
        "nodemon": "^3.1.9",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    }
